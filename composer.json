{
    "name": "mhs/think-addons",
    "description": "thinkphp 6 addons",
    "minimum-stability": "stable",
    "license": "Apache-2.0",
    "version": "0.0.7",
    "authors": [
        {
            "name": "mhs",
            "email": "mhongshan@163.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "topthink/framework": "^6.0.2",
        "topthink/think-helper": "^3.1.4"
    },
    "autoload": {
        "psr-4": {
            "mhs\\think\\": "src"
        },
        "files": [
            "src/common.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "mhs\\think\\service\\AddonsService"
            ],
            "config":{
                "addons": "src/config.php"
            }
        }
    }
}